{
  "$schema": "export_preset_v1",
  "id": "A2_Contour_Minimal_v1",
  "version": 1,
  "display_name": "A2 Konturkarta Minimal",
  "description": "Minimalistisk konturkarta med fokus pa hojdlinjer",

  "bbox_preset": "stockholm_core",
  "theme": "ink",

  "paper": {
    "format": "A2",
    "orientation": "portrait",
    "width_mm": 420,
    "height_mm": 594
  },

  "render": {
    "dpi": 150,
    "format": "png",
    "render_mode": "print"
  },

  "layers": {
    "hillshade": false,
    "water": true,
    "parks": false,
    "roads": false,
    "buildings": false,
    "contours": true
  },

  "composition": {
    "title": null,
    "subtitle": null,
    "show_attribution": true,
    "show_scale_bar": false
  },

  "constraints": {
    "dpi_locked": true,
    "dpi_min": 150,
    "dpi_max": 150,
    "format_locked": false,
    "allowed_formats": ["png", "pdf"],
    "layers_locked": true,
    "bbox_locked": true,
    "theme_locked": true
  },

  "meta": {
    "created": "2025-12-27",
    "author": "system",
    "use_case": "contour_art",
    "tags": ["print", "contour", "minimal", "a2", "art"]
  }
}
