{
  "$schema": "export_preset_v1",
  "id": "A2_Scandi_Light_v1",
  "version": 1,
  "display_name": "A2 Skandinavisk Ljus",
  "description": "Ljus minimalistisk karta med skandinavisk estetik",

  "bbox_preset": "stockholm_core",
  "theme": "warm-paper",

  "paper": {
    "format": "A2",
    "orientation": "landscape",
    "width_mm": 594,
    "height_mm": 420
  },

  "render": {
    "dpi": 150,
    "format": "png",
    "render_mode": "print"
  },

  "layers": {
    "hillshade": true,
    "water": true,
    "parks": true,
    "roads": true,
    "buildings": true,
    "contours": false
  },

  "composition": {
    "title": null,
    "subtitle": null,
    "show_attribution": true,
    "show_scale_bar": false
  },

  "constraints": {
    "dpi_locked": true,
    "dpi_min": 150,
    "dpi_max": 150,
    "format_locked": false,
    "allowed_formats": ["png", "pdf"],
    "layers_locked": true,
    "bbox_locked": true,
    "theme_locked": true
  },

  "meta": {
    "created": "2025-12-27",
    "author": "system",
    "use_case": "scandinavian_interior",
    "tags": ["print", "scandi", "light", "a2", "minimal"]
  }
}
