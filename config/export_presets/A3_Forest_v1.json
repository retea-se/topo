{
  "$schema": "export_preset_v1",
  "id": "A3_Forest_v1",
  "version": 1,
  "display_name": "A3 Forest",
  "description": "Autumn forest palette with earthy greens, browns, and golden tones.",

  "bbox_preset": "stockholm_wide",
  "theme": "forest",

  "paper": {
    "format": "A3",
    "orientation": "landscape",
    "width_mm": 420,
    "height_mm": 297
  },

  "render": {
    "dpi": 150,
    "format": "png",
    "render_mode": "print"
  },

  "layers": {
    "hillshade": true,
    "water": true,
    "parks": true,
    "roads": true,
    "buildings": true,
    "contours": true
  },

  "composition": {
    "title": null,
    "subtitle": null,
    "show_attribution": true,
    "show_scale_bar": true
  },

  "constraints": {
    "dpi_locked": false,
    "dpi_min": 72,
    "dpi_max": 300,
    "format_locked": false,
    "allowed_formats": ["png", "pdf"],
    "layers_locked": false,
    "bbox_locked": false,
    "theme_locked": true
  },

  "meta": {
    "created": "2025-12-27",
    "author": "system",
    "use_case": "nature",
    "tags": ["print", "forest", "autumn", "nature", "earthy", "a3"]
  }
}
