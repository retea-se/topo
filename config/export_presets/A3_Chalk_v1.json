{
  "$schema": "export_preset_v1",
  "id": "A3_Chalk_v1",
  "version": 1,
  "display_name": "A3 Chalk",
  "description": "Chalk on blackboard aesthetic. Educational and nostalgic feel.",

  "bbox_preset": "stockholm_wide",
  "theme": "chalk",

  "paper": {
    "format": "A3",
    "orientation": "portrait",
    "width_mm": 297,
    "height_mm": 420
  },

  "render": {
    "dpi": 150,
    "format": "png",
    "render_mode": "print"
  },

  "layers": {
    "hillshade": true,
    "water": true,
    "parks": false,
    "roads": true,
    "buildings": true,
    "contours": true
  },

  "composition": {
    "title": null,
    "subtitle": null,
    "show_attribution": true,
    "show_scale_bar": true
  },

  "constraints": {
    "dpi_locked": false,
    "dpi_min": 72,
    "dpi_max": 300,
    "format_locked": false,
    "allowed_formats": ["png", "pdf"],
    "layers_locked": false,
    "bbox_locked": false,
    "theme_locked": true
  },

  "meta": {
    "created": "2025-12-27",
    "author": "system",
    "use_case": "educational",
    "tags": ["print", "chalk", "blackboard", "educational", "a3"]
  }
}
